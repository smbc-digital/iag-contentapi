{
  "Contentful:UsePreviewAPI": false,
  "redisExpiryTimes": {
    "News": 60,
    "Events": 60,
    "Groups": 60,
    "Articles": 60,
    "AtoZ": 60
  },
  "ElasticSearchAwsConfiguration": {
    "IndexFormat": "iag-contentapi-qa-{0:yyyy.MM-dd}",
    "MinimumLevel": "Warning",
    "Enabled": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Program Files\\Amazon\\ElasticBeanstalk\\logs\\iag-contentapi-serilog-{Date}.log",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
