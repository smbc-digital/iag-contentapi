{
  "secrets-location": "c:\\secrets\\contentapi",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ButoBaseUrl": "https://api.buto.tv/v2/",
  "RedirectBusinessIds": [
    "stockportgov"//,
 //   "healthystockport"
  ],
  "ElasticSearch": {
    "Enabled": false,
    "LogLevel": "Debug",
    "LogFormat": "contentapilogs-{0:yyyy.MM.dd}"
  },
  "ClientRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "HttpStatusCode": 429,
    "EndpointWhitelist": [ ],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 2
      },
      {
        "Endpoint": "*",
        "Period": "15m",
        "Limit": 100
      },
      {
        "Endpoint": "*",
        "Period": "12h",
        "Limit": 1000
      },
      {
        "Endpoint": "*",
        "Period": "7d",
        "Limit": 10000
      }
    ]
  }
}