{
  "secrets-location": "c:\\secrets\\contentapi",
  "UseRedisSessions": "false",
  "redisExpiryTimes": {
    "News": 60,
    "Events": 60,
    "Groups": 60,
    "Articles": 0,
    "AtoZ": 60
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "TwentyThreeBaseUrl": "https://y84kj.videomarketingplatform.co/v.ihtml/player.html?source=embed&photo%5fid=",
  "RedirectBusinessIds": [
    "stockportgov",
    "healthystockport"
  ],
  "ElasticSearchAwsConfiguration": {
    "Region": "eu-west-1",
    "IndexFormat": "iag-contentapi-local-{0:yyyy.MM-dd}",
    "InlineFields": false,
    "MinimumLevel": "Warning",
    "Enabled": true,
    "Url": "https://search-stockport-logs-4evarozpqvlofjsnxzliq7eqdy.eu-west-1.es.amazonaws.com/"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File",
      "Serilog.Sinks.Console",
      "Serilog.Enrichers.Environment",
      "Serilog.Enrichers.Thread",
      "Serilog.Enrichers.Process"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId",
      "WithProcessId"
    ]
  },
  "ClientRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "HttpStatusCode": 429,
    "EndpointWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 2
      },
      {
        "Endpoint": "*",
        "Period": "15m",
        "Limit": 100
      },
      {
        "Endpoint": "*",
        "Period": "12h",
        "Limit": 1000
      },
      {
        "Endpoint": "*",
        "Period": "7d",
        "Limit": 10000
      }
    ]
  },
  "Contentful": {
    "DeliveryUrl": "https://cdn.contentful.com",
    "UsePreviewAPI": false
  }
}